# :command.parse_requirements
art_scene_add_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --help | -h)
        long_usage=yes
        art_scene_add_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="scene add"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        # :argument.case
        if [[ -z ${args['scene']+x} ]]; then
          args['scene']=$1
          shift
        # :argument.case
        elif [[ -z ${args['subscene']+x} ]]; then
          args['subscene']=$1
          shift
        # :argument.case
        elif [[ -z ${args['source']+x} ]]; then
          args['source']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['scene']+x} ]]; then
    printf "missing required argument: SCENE\nusage: art scene add SCENE SUBSCENE SOURCE\n" >&2

    exit 1
  fi
  if [[ -z ${args['subscene']+x} ]]; then
    printf "missing required argument: SUBSCENE\nusage: art scene add SCENE SUBSCENE SOURCE\n" >&2

    exit 1
  fi
  if [[ -z ${args['source']+x} ]]; then
    printf "missing required argument: SOURCE\nusage: art scene add SCENE SUBSCENE SOURCE\n" >&2

    exit 1
  fi

  # :command.validations
  # :argument.validations
  if [[ -v args['scene'] && -n $(validate_scene_exists "${args['scene']:-}") ]]; then
    printf "validation error in %s:\n%s\n" "SCENE" "$(validate_scene_exists "${args['scene']:-}")" >&2
    exit 1
  fi

  # :argument.validations
  if [[ -v args['source'] && -n $(validate_file_exists "${args['source']:-}") ]]; then
    printf "validation error in %s:\n%s\n" "SOURCE" "$(validate_file_exists "${args['source']:-}")" >&2
    exit 1
  fi

}

# :command.parse_requirements
art_scene_rm_parse_requirements() {
  # :command.fixed_flags_filter
  while [[ $# -gt 0 ]]; do
    case "${1:-}" in
      --help | -h)
        long_usage=yes
        art_scene_rm_usage
        exit
        ;;

      *)
        break
        ;;

    esac
  done

  # :command.command_filter
  action="scene rm"

  # :command.parse_requirements_while
  while [[ $# -gt 0 ]]; do
    key="$1"
    case "$key" in

      -?*)
        printf "invalid option: %s\n" "$key" >&2
        exit 1
        ;;

      *)
        # :command.parse_requirements_case
        # :command.parse_requirements_case_simple
        # :argument.case
        if [[ -z ${args['scene']+x} ]]; then
          args['scene']=$1
          shift
        # :argument.case
        elif [[ -z ${args['subscene']+x} ]]; then
          args['subscene']=$1
          shift
        else
          printf "invalid argument: %s\n" "$key" >&2
          exit 1
        fi

        ;;

    esac
  done

  # :command.required_args_filter
  if [[ -z ${args['scene']+x} ]]; then
    printf "missing required argument: SCENE\nusage: art scene rm SCENE SUBSCENE\n" >&2

    exit 1
  fi
  if [[ -z ${args['subscene']+x} ]]; then
    printf "missing required argument: SUBSCENE\nusage: art scene rm SCENE SUBSCENE\n" >&2

    exit 1
  fi

  # :command.validations
  # :argument.validations
  if [[ -v args['scene'] && -n $(validate_scene_exists "${args['scene']:-}") ]]; then
    printf "validation error in %s:\n%s\n" "SCENE" "$(validate_scene_exists "${args['scene']:-}")" >&2
    exit 1
  fi

}

# :command.initialize
initialize() {
  version="0.1.0"
  long_usage=''
  set -e

  # :command.environment_variables_default
  export ART_CURRENT_PATH="${ART_CURRENT_PATH:-$HOME/Art}"
  export ART_PROJECTS_PATH="${ART_PROJECTS_PATH:-$HOME/Videos/Creations}"

  # src/initialize.sh
  mkdir -p "$ART_PROJECTS_PATH"

}

# :command.run
run() {
  declare -A args=()
  declare -A deps=()
  declare -a other_args=()
  declare -a env_var_names=()
  declare -a input=()
  normalize_input "$@"
  parse_requirements "${input[@]}"

  case "$action" in
    "list") art_list_command ;;
    "init") art_init_command ;;
    "select") art_select_command ;;
    "scene") art_scene_command ;;
    "scene list") art_scene_list_command ;;
    "scene create") art_scene_create_command ;;
    "scene add") art_scene_add_command ;;
    "scene rm") art_scene_rm_command ;;
  esac
}

initialize
run "$@"
